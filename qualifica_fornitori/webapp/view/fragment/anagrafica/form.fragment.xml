<c:FragmentDefinition

xmlns="sap.m"
xmlns:c="sap.ui.core"
xmlns:f="sap.ui.layout.form"
>
<f:SimpleForm 
editable="false"
  layout="ResponsiveGridLayout"
  emptySpanXL="4"
  emptySpanL="1"
  emptySpanM="0"
  emptySpanS="0"
  columnsXL="1"
  columnsL="1"
  columnsM="1"
  singleContainerFullSize="false" 
>
  <f:content>
    <Label text="Nome" />
    <Input value="{anagraficaModel>/0/nome}" editable="false"/>
    <Label text="Via" />
    <Input value="{anagraficaModel>/0/indirizzo/via}" editable="false"/>
    <Label text="CAP" />
    <Input value="{anagraficaModel>/0/indirizzo/cap}" editable="false"/>
    <Label text="Comune" />
    <Input value="{anagraficaModel>/0/indirizzo/comune}" editable="false"/>
    <Label text="Provicia" />
    <Input value="{anagraficaModel>/0/indirizzo/provincia}" editable="false"/>
    <Label text="Stato" />
    <Input value="{anagraficaModel>/0/indirizzo/stato}" editable="false"/>
    <Label text="Lingua" />
    <Input value="{anagraficaModel>/0/lingua}" editable="false"/>
    <Label text="Telefono" />
    <Input value="{anagraficaModel>/0/contatti/telefono/interno}" editable="false"/>
    <Label text="Fax" />
    <Input value="{anagraficaModel>/0/contatti/fax/interno}" editable="false"/>
    <Label text="Email" />
    <Input value="{anagraficaModel>/0/contatti/email}" editable="false"/>
    <Label text="P.IVA" />
    <Input value="{anagraficaModel>/0/partita_iva}" editable="false"/>
    <!-- <Label text="Divisa" />
    <Input/>
    <Label text="Cond. Pagamento" />
    <Input/>
    <Label text="Incoterms" />
    <Input/> -->
    
  </f:content>
</f:SimpleForm>


</c:FragmentDefinition>